{
  "name": "advanced-clinic",
  "version": "1.0.0",
  "description": "This is an advanced hospital management system",
  "main": "app.js",
  "scripts": {
    "start": "nodemon server.js",
    "create": "sequelize db:create",
    "drop": "sequelize db:drop",
    "migration": "sequelize db:migrate",
    "seedAll": "sequelize db:seed:all",
    "migration:undo": "sequelize db:migrate:undo:all",
    "undoseed": "sequelize db:seed:undo:all",
    "create:model": "sequelize model:generate --name",
    "create:seed": "sequelize seed:generate --name "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jeanndo/advanced-clinic.git"
  },
